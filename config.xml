<?xml version="1.0" encoding="UTF-8" ?>
<widget xmlns = "http://www.w3.org/ns/widgets"
    xmlns:gap = "http://phonegap.com/ns/1.0"
    id        = "com.mascotasvqrlibsanitaria.app"
    version   = "1.0.1">

<name>MascotasVQR</name>

<description>
Con esta aplicación tanto vos como tu veterinario van a poder actualizar la libreta sanitaria de tu mascota en todo momento, en cualquier lugar. MascotasVQR es innovación constante.
</description>


<preference name="phonegap-version" value="3.6.3" />

<gap:plugin name="org.apache.cordova.device" version="0.2.12" />
<gap:plugin name="org.apache.cordova.dialogs" version="0.2.10" />

<preference name="fullscreen" value="true" />
<icon src="img/icon_gde.png" />
<gap:splash src="img/splash.png" />
<preference name="splash-screen-duration" value="20000" />
<preference name="exit-on-suspend" value="true" />
<preference name="android-minSdkVersion" value="5" />
<preference name="android-targetSdkVersion" value="19" />
<preference name="android-maxSdkVersion" value="19" />
<preference name="permissions" value="none" />

<author href="http://www.mascotasvqr.com" email="contacto@mascotasvqr.com">
@leocondori
</author>

</widget>
